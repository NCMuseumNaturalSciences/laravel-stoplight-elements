{
    "name": "ncmns/laravel-stoplight-elements",
    "description": "An extended API documentation package for Laravel using OpenAPI and Stoplight Elements",
    "keywords": [
        "laravel",
        "api",
        "documentation",
        "stoplight",
        "stoplight elements",
        "openapi"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "developer",
            "name": "Ben Norton",
            "email": "ben.norton@naturalsciences.org",
            "homepage": "https://naturalsciences.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "NCMuseumNaturalSciences\\Laravel\\LaravelStoplight\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NCMuseumNaturalSciences\\Laravel\\LaravelStoplight\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NCMuseumNaturalSciences\\Laravel\\LaravelStoplight\\StoplightServiceProvider"
            ]
        }
    }
}
